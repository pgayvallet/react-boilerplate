{
  "name": "react-boilerplate",
  "version": "0.0.1",
  "scripts": {
    "clean"     : "rimraf ./dist/* && rimraf ./build/*",
    "start"     : "cross-env NODE_ENV=development node webpack/dev-server.js",
    "build"     : "cross-env NODE_ENV=development webpack --config webpack.config.js",
    "package"   : "cross-env NODE_ENV=production webpack --config webpack.config.js",
    "test"      : "jest",
    "lint:js"   : "eslint ./src",
    "precommit" : "npm run lint:js",
    "prepush"   : "npm run test"
  },
  "moduleRoots": [
    "src"
  ],
  "dependencies": {

    "lodash": "4.17.4",
    "moment": "2.18.1",
    "numeral": "2.0.6",
    "uuid" : "3.1.0",

    "font-awesome" : "4.7.0",

    "stackframe" : "1.0.3",
    "error-stack-parser" : "2.0.1",

    "jquery": "3.2.1",

    "axios" : "0.16.2",
    "redux" : "3.7.2",
    "react-redux" : "5.0.5",
    "redux-thunk" : "2.2.0",
    "redux-saga" : "0.15.6",
    "reselect": "3.0.1",
    "classnames": "2.2.5",
    "react" : "15.6.1",
    "react-dom" : "15.6.1",
    "react-router-dom" : "4.1.2",
    "react-router-redux" : "5.0.0-alpha.6",
    "history" : "4.6.3"
  },
  "devDependencies": {

    "husky" : "0.14.3",
    "rimraf" : "2.6.1",
    "better-npm-run" : "0.1.0",
    "cross-env" : "5.0.2",
    "webpack": "3.4.1",
    "webpack-dev-server" : "2.6.1",
    "happypack" : "3.1.0",

    "css-loader": "0.28.4",
    "sass-loader": "6.0.6",
    "style-loader" : "0.18.2",
    "raw-loader": "0.5.1",
    "html-loader" : "0.5.0",
    "exports-loader" : "0.6.4",
    "expose-loader" : "0.7.3",
    "imports-loader" : "0.7.1",
    "file-loader" : "0.11.2",
    "postcss-loader" : "2.0.6",
    "null-loader" : "0.1.1",
    "react-hot-loader" : "3.0.0-beta.6",

    "flow-bin": "^0.52.0",
    "jest": "^20.0.4",
    "jest-cli": "^20.0.4",
    "babel-jest": "20.0.3",
    "identity-obj-proxy" : "3.0.0",

    "eslint" : "4.1.1",
    "eslint-config-standard": "^10.2.1",
    "eslint-config-standard-react": "^5.0.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.3.0",
    "eslint-plugin-react" : "7.1.0",
    "eslint-plugin-jsx-a11y": "6.0.2",
    "eslint-plugin-standard": "3.0.1",

    "babel-core" : "6.25.0",
    "babel-loader" : "7.1.1",
    "babel-runtime" : "6.25.0",
    "babel-preset-es2015" : "6.24.1",
    "babel-preset-flow" : "6.23.0",
    "babel-preset-react": "6.24.1",
    "babel-plugin-transform-runtime" : "6.23.0",
    "babel-plugin-transform-object-rest-spread" : "6.23.0",
    "babel-plugin-lodash": "3.2.10",

    "node-sass": "4.5.3",
    "autoprefixer" : "7.1.2",
    "cssnano" : "3.10.0",

    "copy-webpack-plugin": "4.0.1",
    "extract-text-webpack-plugin": "3.0.0",
    "html-webpack-plugin" : "2.30.1"
  },
  "engines": {
    "node": ">=6.0.0"
  }
}
